{
  "name": "Biscuits",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/ebith/Biscuits",
  "author": "ebith <ebith.h@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-purgecss": "^2.1.0",
    "preact": "^8.4.2",
    "sass": "^1.17.0"
  },
  "xo": {
    "space": true,
    "prettier": true,
    "plugins": [
      "react"
    ],
    "rules": {
      "capitalized-comments": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1
    },
    "env": [
      "browser"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "iOS 12"
  ],
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html --no-source-maps --no-cache --experimental-scope-hoisting",
    "dev": "node dev.js"
  }
}
